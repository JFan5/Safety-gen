set -euo pipefail

conda activate llmstl

SCENARIO="blocksworld"
VARIANT="pddl3"

mkdir -p "/groups/fkong/jfan5/data/dpo/mistral_unsloth/blocksworld/pddl3"

python3 script/generate_score_candidate.py \
  --model "unsloth/mistral-7b-instruct-v0.3-bnb-4bit" \
  --family mistral \
  --domain-file "blocksworld/domain3.pddl" \
  --problems-dir "blocksworld/all_problems3/training" \
  --prompt-template "prompt.txt" \
  --temperature 0.7 \
  --top-p 0.9 \
  --top-k 50 \
  --seed 42 \
  --max-new-tokens 2000 \
  --out "/groups/fkong/jfan5/data/dpo/mistral_unsloth/blocksworld/pddl3/scored.jsonl"

echo "${SCENARIO} (${VARIANT^^}) candidate generation complete for mistral_unsloth."
