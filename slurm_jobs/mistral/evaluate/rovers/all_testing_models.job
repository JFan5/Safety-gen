#!/bin/bash

#SBATCH --mail-user=jfan5@nd.edu
#SBATCH --mail-type=BEGIN,END,FAIL
#SBATCH --partition=gpu
#SBATCH --gres=gpu:1
#SBATCH --output=job_outputs/evaluate_rovers_all_testing_models.o
#SBATCH --cpus-per-task=28
#SBATCH --job-name=evaluate_rovers_all_testing_models

conda activate llmstl

# PDDL2 fine-tuned model
python3 script/evaluate_llm_solver.py \
  --model sft_model/mistral_7b/rovers/pddl2 \
  --problems-dir rovers/all_problems3/testing \
  --domain-file rovers/domain3.pddl \
  --output rovers_all_testing_pddl2.json \
  --results-dir planning_results/sft_mistral_7b/rovers/pddl2

# PDDL3 fine-tuned model evaluated on PDDL2 testing set
python3 script/evaluate_llm_solver.py \
  --model sft_model/mistral_7b/rovers/pddl3 \
  --problems-dir rovers/all_problems3/testing \
  --domain-file rovers/domain3.pddl \
  --output rovers_all_testing_pddl3.json \
  --results-dir planning_results/sft_mistral_7b/rovers/pddl3

# Baseline unsloth model
python3 script/evaluate_llm_solver.py \
  --model unsloth/mistral-7b-instruct-v0.3-bnb-4bit \
  --problems-dir rovers/all_problems3/testing \
  --domain-file rovers/domain3.pddl \
  --output rovers_all_testing_baseline.json \
  --results-dir planning_results/unsloth_mistral-7b-instruct-v0.3-bnb-4bit/rovers/baseline
